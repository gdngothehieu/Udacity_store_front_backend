{
  "name": "Udacity_store_front_backend",
  "version": "0.1.0",
  "description": "## Getting Started",
  "main": "server.ts",
  "scripts": {
    "build-window": "rmdir dist | echo 'Y' &&  npx tsc",
    "build-linux": "rm -r dist && npx tsc",
    "start": "ts-node src/server.ts",
    "dev": "npm run build-window && tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "delete_TEST_DB": "db-migrate db:drop store_front_test",
    "migrate_up": "db-migrate up --env test",
    "test": "npm run delete_TEST_DB && db-migrate db:create abc --env create && npm run migrate_up && npm run build-window && jasmine && npm run delete_TEST_DB"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.11",
    "jasmine": "^4.0.2",
    "jasmine-spec-reporter": "^7.0.0",
    "supertest": "^6.2.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  },
  "keywords": []
}
